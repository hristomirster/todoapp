version: '3.8'
services:
 frontend:
  build: ./frontend
  ports:
   - 3000:3000
  networks:
   - react-express

 backend:
  build: ./backend
  networks:
  - react-express
  - express-mongo

 mongo:
  image: mongo:latest
  volumes:
   - ./data:/data/db
  networks:
   - express-mongo

networks:
  react-express:
  express-mongo:
    